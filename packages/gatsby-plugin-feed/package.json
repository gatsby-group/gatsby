  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "deepmerge": "^2.0.1",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.get": "^4.4.2",
    "pify": "^3.0.0",
    "rss": "^1.2.2"
