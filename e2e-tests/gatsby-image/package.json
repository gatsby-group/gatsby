{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "cypress": "^3.8.3",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-manifest": "^2.2.38",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "test": "cross-env CYPRESS_SUPPORT=y npm run build && npm run start-server-and-test",
    "start-server-and-test": "start-server-and-test serve http://localhost:9000 cy:run",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "(is-ci && cypress run --browser chrome --record) || cypress run --browser chrome"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "gatsby-cypress": "^0.2.21",
    "is-ci": "^2.0.0",
    "start-server-and-test": "^1.10.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
