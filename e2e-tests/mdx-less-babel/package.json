{
  "name": "gatsby-mdx-tester",
  "description": "Gatsby MDX tester",
  "version": "1.0.0",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "cypress": "^3.1.0",
    "gatsby": "^3.0.0",
    "gatsby-plugin-mdx": "^2.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "theme-ui": "^0.3.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "cross-env CYPRESS_SUPPORT=y gatsby build",
    "develop": "cross-env CYPRESS_SUPPORT=y gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "npm run build && npm run start-server-and-test",
    "start-server-and-test": "start-server-and-test serve http://localhost:9000 cy:run",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "node ../../scripts/cypress-run-with-conditional-record-flag.js --browser chrome"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "gatsby-cypress": "^0.1.7",
    "is-ci": "^2.0.0",
    "prettier": "2.0.4",
    "start-server-and-test": "^1.7.1"
  }
}
